<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nag_graph"
    app:startDestination="@id/jokeFragment">

    <fragment
        android:id="@+id/jokeFragment"
        android:name="com.example.randomjoke.joke.JokeFragment"
        android:label="fragment_joke_screen"
        tools:layout="@layout/fragment_joke_screen" >
        <action
            android:id="@+id/action_jokeFragment_to_jokeListFragment"
            app:destination="@id/jokeListFragment" />
    </fragment>
    <fragment
        android:id="@+id/jokeListFragment"
        android:name="com.example.randomjoke.jokelist.JokeListFragment"
        android:label="fragment_jokes_list"
        tools:layout="@layout/fragment_jokes_list" />
</navigation>